<?xml version='1.0' encoding='UTF-8'?>
<faces-config
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://xmlns.jcp.org/xml/ns/javaee"
		xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
		version="2.2">
	<navigation-rule>
		<navigation-case>
			<from-outcome>persisted</from-outcome>
			<to-view-id>/customer/main.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<navigation-case>
			<from-outcome>updated</from-outcome>
			<to-view-id>/customer/main.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<navigation-case>
			<from-outcome>removed</from-outcome>
			<to-view-id>/customer/main.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<navigation-case>
			<from-outcome>create</from-outcome>
			<to-view-id>/customer/create.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>

</faces-config>